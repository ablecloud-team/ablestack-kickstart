[Unit]
Description=Process Exporter
After=network-online.target

[Service]
User=root
Restart=on-failure

#Change this line if you download ther

ExecStart=/usr/share/ablestack/exporter/process-exporter/process-exporter \
  -web.listen-address ":3004" \
  -config.path /usr/share/ablestack/exporter/process-exporter/process.yml

[Install]
WantedBy=multi-user.target